<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.com.yongda.repository.dao.GatewayItfDao">
   <select id="queryGatewayItf" resultType="cn.com.yongda.repository.entity.GatewayItf">
      select ITF_ID,ITF_NAME,
      ITF_TYPE,GMT_CREATE,GMT_MODIFIED,REMARK
      from T_GATEWAY_ITF
      where ITF_ID=#{itfId}
   </select>
</mapper>